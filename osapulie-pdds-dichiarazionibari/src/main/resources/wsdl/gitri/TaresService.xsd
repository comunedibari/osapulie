<?xml version='1.0' encoding='UTF-8'?><!-- Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is JAX-WS RI 2.2.8 svn-revision#13980. --><xs:schema xmlns:tns="http://www.comune.bari.it/serviziTributi" xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0" targetNamespace="http://www.comune.bari.it/serviziTributi">

<xs:element name="getUtenze" type="tns:getUtenzeType"/>

<xs:element name="getUtenzeResponse" type="tns:getUtenzeResponseType"/>

<xs:element name="getUtenzeStatoPraticheRequest" type="tns:getUtenzeStatoPraticheRequestType"/>

<xs:element name="getUtenzeStatoPraticheResponse" type="tns:getUtenzeStatoPraticheResponseType"/>

<xs:element name="isContribuenteEsistenteRequest" type="tns:isContribuenteEsistenteRequestType"/>

<xs:element name="isContribuenteEsistenteResponse" type="tns:isContribuenteEsistenteResponseType"/>

<xs:element name="setAgevolazioniCAF" type="tns:setAgevolazioniCAFType"/>

<xs:element name="setAgevolazioniCAFResponse" type="tns:setAgevolazioniCAFResponseType"/>

<xs:element name="setCessazioneRequest" type="tns:setCessazioneRequestType"/>

<xs:element name="setCessazioneResponse" type="tns:setCessazioneResponseType"/>

<xs:element name="setIscrizioneRequest" type="tns:setIscrizioneRequestType"/>

<xs:element name="setIscrizioneResponse" type="tns:setIscrizioneResponseType"/>

<xs:element name="setRiduzioneRequest" type="tns:setRiduzioneRequestType"/>

<xs:element name="setRiduzioneResponse" type="tns:setRiduzioneResponseType"/>

<xs:element name="setTrasferimentoRequest" type="tns:setTrasferimentoRequestType"/>

<xs:element name="setTrasferimentoResponse" type="tns:setTrasferimentoResponseType"/>

<xs:element name="setVariazioneCategoriaRequest" type="tns:setVariazioneCategoriaRequestType"/>

<xs:element name="setVariazioneCategoriaResponse" type="tns:setVariazioneCategoriaResponseType"/>

<xs:element name="setVariazioneSuperficieRequest" type="tns:setVariazioneSuperficieRequestType"/>

<xs:element name="setVariazioneSuperficieResponse" type="tns:setVariazioneSuperficieResponseType"/>

<xs:complexType name="setIscrizioneRequestType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per la dichiarazione di iscrizione di una nuova utenza</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:commonRequestType">
<xs:sequence>
<xs:element name="datiIscrizione" type="tns:datiIscrizioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della dichiarazione di iscrizione di una nuova utenza</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="commonRequestType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati comuni di una richiesta di un servizio web Tares</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="datiAutorizzazione" type="tns:datiAutorizzazioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati necessari alla verifica dell'autorizzazione del sistema chiamante all'accesso al servizio richiesto</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiTracciamento" type="tns:datiTracciamentoRequestType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati necessari al tracciamento della richiesta</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiIscrizioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per una dichiarazione d'iscrizione di una nuova utenza</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:datiDichiarazioneBaseType">
<xs:sequence>
<xs:element name="datiContribuente" type="tns:datiContribuenteIscrizioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati del contribuente dichiarante. Se contribuente esistente allora specificare idContribuente, altrimenti specificare contribuenteUtenzeCommerciali o contribuenteUtenzeDomestiche per i dati del nuovo contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiUtenze" type="tns:datiUtenzeType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della/e utenza/e oggetto d'iscrizione</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="datiDichiarazioneBaseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso di base per una dichiarazione riguardante un'utenza Tares</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="dataDichiarazione" type="tns:dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Data di presentazione della dichiarazione nel formato gg/mm/aaaa</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="numProtocollo" type="tns:numProtocolloType">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di protocollazione della dichiarazione</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="dataProtocollo" type="tns:dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Data di protocollazione della dichiarazione</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="allegati" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione dell'eventuale documentazione allegata alla dichiarazione</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="500"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="docIdentita">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione degli estremi del documento d'identità del contribuente</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="500"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="delegato" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione degli estremi anagrafici della persona delegata dal contribuente alla presentazione della dichiarazione</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="500"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="note" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Annotazioni</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="400"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="tipoUtenza" type="tns:tipoUtenzaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipologia di utenza/e oggetto della dichiarazione: C = commerciale, D = domestica</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiContribuenteIscrizioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un contribuente ai fini della dichiarazione d'iscrizione di una nuova utenza. Se contribuente esistente allora specificare idContribuente, altrimenti specificare contribuenteUtenzeCommerciali o contribuenteUtenzeDomestiche per i dati del nuovo contribuente</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:choice>
<xs:element name="idContribuente" type="tns:identificativoContribuenteType"/>
<xs:element name="nuovoContribuente" type="tns:contribuenteType"/>
</xs:choice>
<xs:element name="datiProvenienza" type="tns:indirizzoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della precedente ubicazione del contribuente. Se comune &amp;egrave; Bari allora specificare obbligatoriamente viaCodificata altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiDomicilio" type="tns:indirizzoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della residenza anagrafica (se persona fisica) o della sede anagrafica (persona giuridica). Se comune è Bari allora specificare obbligatoriamente viaCodificata altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiRappresentante" type="tns:datiRappresentanteType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della persona fisica rappresentante legalmente una persona giuridica. Da specificare solo in caso di contribuenti per utenze di natura commerciale. E' obbligatorio specificarli in caso di nuovo contribuente di tipo persona giuridica, mentre è possibile specificarli, se variati, in caso di esistenza del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiRecapito" type="tns:datiRecapitoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati dell'eventuale recapito (differente dalla residenza anagrafica o sede legale) del contribuente per la ricezione delle comunicazioni. Se comune è Bari allora specificare obbligatoriamente viaCodificata altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="indirizzoPEC" type="tns:emailType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Indirizzo di Posta Elettronica Certificata (P.E.C.) del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="telefono" type="tns:telefonoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di telefono di contatto del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="fax" type="tns:telefonoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di fax di contatto del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="identificativoContribuenteType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per l'identificativo univoco di un contribuente</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:int">
<xs:maxInclusive value="999999"/>
<xs:minInclusive value="1"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="contribuenteType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un contribuente (persona fisica o giuridica) dichiarante utenze domestiche o commerciali</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="provinciaREA" type="tns:siglaProvinciaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Provincia di registrazione al Repertorio delle notizie Economiche e Ammninistrative (R.E.A.). Da specificare obbligatoriamente per contribuenti di tipo giuridico (Società)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="numeroREA" type="tns:numeroReaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di registrazione al Repertorio delle notizie Economiche e Ammninistrative (R.E.A.). Da specificare obbligatoriamente per contribuenti di tipo giuridico (Società)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="partitaIVA" type="tns:partitaIVAType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Partita I.V.A. dell'attività economica. Da specificare obbligatoriamente per contribuenti di tipo giuridico (Società e ditte individuali)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice>
<xs:element name="personaFisica" type="tns:personaFisicaType"/>
<xs:element name="personaGiuridica" type="tns:personaGiuridicaType"/>
</xs:choice>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="siglaProvinciaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la sigla di una provincia italiana</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[A-Z]{2}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="numeroReaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un numero di registrazione al Repertorio delle notizie Economiche e Ammninistrative (R.E.A.)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:long">
<xs:maxInclusive value="9999999999"/>
<xs:minInclusive value="1"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="partitaIVAType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la partita IVA</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{11}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="personaFisicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati anagrafici di una persona fisica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="cognome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="30"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="nome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="24"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="comuneNascita" type="tns:comuneNascitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Comune di nascita codificato tramite il codice ISTAT o se nato all'estero codifica ISTAT dello stato estero</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="comuneEsteroNascita" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione del comune estero di nascita. Da specificare solo se nato all'estero</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="30"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="provinciaNascita" type="tns:siglaProvinciaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Provincia di nascita del contribuente. Sarà presente solo se nato in Italia</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="dataNascita" type="tns:dataType"/>
<xs:element name="sesso" type="tns:sessoType"/>
<xs:element name="codiceFiscale" type="tns:codiceFiscaleType"/>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="comuneNascitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il comune di nascita di una persona fisica codificato tramite il codice ISTAT dei comuni italiani o tramite codice ISTAT degli stati esteri (se nato all'estero)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{6}|[0-9]{3}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una data nel formato gg/mm/aaaa</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="(0[1-9]|[12][0-9]|3[01])/(0[1-9]|1[012])/(19[0-9]{2}|20[0-9]{2})"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="codiceFiscaleType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il codice fiscale di una persona fisica</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[a-zA-Z]{6}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){3}[a-zA-Z]{1}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="personaGiuridicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati anagrafici di una persona giuridica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="denominazione">
<xs:annotation>
<xs:documentation xml:lang="it">Denominazione o ragione sociale</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="84"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="codiceFiscale" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Codice fiscale della persona giuridica</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{11}|[a-zA-Z]{6}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){3}[a-zA-Z]{1}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="indirizzoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per un indirizzo completo</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="comune" type="tns:codiceCatastaleComuneType">
<xs:annotation>
<xs:documentation xml:lang="it">Codice catastale del comune</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice>
<xs:element name="viaCodificata" type="tns:viaCodificataType"/>
<xs:element name="viaNonCodificata" type="tns:viaNonCodificataType"/>
</xs:choice>
<xs:element name="civico" type="tns:civicoType" minOccurs="0"/>
<xs:element name="esponente" type="tns:esponenteCivicoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Esponente del numero civico</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="piano" type="tns:pianoType" minOccurs="0"/>
<xs:element name="interno" type="tns:internoType" minOccurs="0"/>
<xs:element name="scala" type="tns:scalaType" minOccurs="0"/>
<xs:element name="suffisso" type="tns:suffissoType" minOccurs="0"/>
<xs:element name="km" type="tns:chilometroType" minOccurs="0"/>
<xs:element name="frazione" type="tns:frazioneType" minOccurs="0"/>
<xs:element name="cap" type="tns:capType" minOccurs="0"/>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="codiceCatastaleComuneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un comune italiano codificato tramite il codice catastale (Belfiore)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[A-L][0-9]{3}|M[0-3]{3}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="viaCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una via del comune di Bari codificata secondo il SIT comunale</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{1,9}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="viaNonCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una via tramite una descrizione testuale</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="35"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="civicoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un numero civico</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:int">
<xs:maxInclusive value="9999"/>
<xs:minInclusive value="0"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="esponenteCivicoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un esponente di un numero civico</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="4"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="pianoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il piano in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="2"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="internoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per l'interno in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="2"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="scalaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la scala in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="2"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="suffissoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il suffisso in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="20"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="chilometroType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per l'indicazione del chilometro in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:decimal">
<xs:fractionDigits value="3"/>
<xs:maxInclusive value="999.999"/>
<xs:totalDigits value="6"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="frazioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la frazione in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="20"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="capType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il Codice di Avviamento Postale (C.A.P.)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{5}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiRappresentanteType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un rappresentante legale di un'attività economica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="cognome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="35"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="nome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="35"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="comuneNascita" type="tns:comuneNascitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Comune di nascita codificato tramite il codice ISTAT o se nato all'estero codifica ISTAT dello stato estero</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="comuneEsteroNascita" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione del comune estero di nascita. Da specificare solo se nato all'estero</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="30"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="provinciaNascita" type="tns:siglaProvinciaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Provincia di nascita del rappresentante. Sarà presente solo se nato in Italia</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="dataNascita" type="tns:dataType"/>
<xs:element name="sesso" type="tns:sessoType"/>
<xs:element name="codiceFiscale" type="tns:codiceFiscaleType"/>
<xs:element name="qualifica">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione della qualifica della persona fisica rappresentante legalmente un'attività economica</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="22"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="indirizzoPEC" type="tns:emailType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Indirizzo di Posta Elettronica Certificata (P.E.C.) del rappresentante</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="telefono" type="tns:telefonoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di telefono di contatto del rappresentante</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="fax" type="tns:telefonoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di fax di contatto del rappresentante</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="emailType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un indirizzo email</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="254"/>
<xs:pattern value="([a-zA-Z0-9]+[\-._+])*[a-zA-Z0-9]+@([\-0-9a-zA-Z]+[.])+[a-zA-Z]{2,6}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="telefonoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato per il sesso di una persona: M o F</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="30"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiRecapitoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di recapito di un contribuente. Se comune è Bari allora specificare viaCodificata altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:indirizzoType">
<xs:sequence>
<xs:element name="presso" type="xs:string" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Denominazione della persona (o altro) intestario dell'indirizzo di recapito</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="datiUtenzeType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati della/e utenza/e oggetto di iscrizione</xs:documentation>
</xs:annotation>
<xs:choice>
<xs:element name="datiUtenzeDomestiche" type="tns:datiUtenzeDomesticheType"/>
<xs:element name="datiUtenzeCommerciali" type="tns:elencoDatiUtenzeCommercialiType"/>
</xs:choice>
</xs:complexType>

<xs:complexType name="datiUtenzeDomesticheType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati delle utenze domestiche associabili ad una pratica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="utenza" type="tns:datiUtenzaDomesticaType" minOccurs="1" maxOccurs="unbounded">
<xs:annotation>
<xs:documentation xml:lang="it">Elenco delle utenze domestiche (almeno una) da associare alla pratica</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneTariffa" type="tns:riduzioneTariffaDomestica2014Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Codice di riduzione tariffa richiesta ai sensi dell'art. 23 del Regolamento Tari. Se presente, è riferita alle utenze domestiche specificate. Possibili valori: A = Abitazioni con un unico occupante, B = Abitazioni tenute a disposizione per uso non superiore a 183 giorni nell'anno solare, C = Abitazioni occupate da soggetti che risiedano o abbiano la dimora all'estero per piu' di 6 mesi all'anno, D = Fabbricati rurali ad uso abitativo</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="contribuenteResidente" type="xs:boolean">
<xs:annotation>
<xs:documentation xml:lang="it">Indica se il contribuente risiede nell'immobile o meno. E' riferito alle utenze domestiche specificate</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="numComponentiNucleoFamiliare">
<xs:annotation>
<xs:documentation xml:lang="it">Numero componenti del nucleo familiare. E' riferito alle utenze domestiche specificate.</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:unsignedByte">
<xs:fractionDigits value="0"/>
<xs:maxInclusive value="20"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="elencoOccupantiImmobile" type="tns:elencoOccupantiImmobileType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Elenco degli occupanti dell'immobile. Se presente, è riferito alle utenze domestiche specificate.</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiUtenzaDomesticaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un'utenza domestica.</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:datiUtenzaBaseType">
<xs:sequence>
<xs:element name="categoriaCatastale" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Categoria catastale dell'immobile</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="tns:categoriaCatastaleType">
<xs:maxLength value="4"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="uso" type="tns:usoImmobileType">
<xs:annotation>
<xs:documentation xml:lang="it">Uso dell'immobile: A = abitazione, P = pertinenza</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="superficie" type="tns:superficieType">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie tassabile in mq</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="datiUtenzaBaseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un'utenza</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="dataDecorrenza" type="tns:dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Data di inizio di detenzione od occupazione dell'immobile</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="ubicazione" type="tns:indirizzoComuneType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati dell'ubicazione (indirizzo) dell'immobile</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiCatastali" type="tns:datiCatastaliNewType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati catastali dell'immobile/locale/area</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="superficieType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una superficie in mq</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:int">
<xs:maxInclusive value="99999999"/>
<xs:minInclusive value="0"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="indirizzoComuneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per un indirizzo completo all'interno del territorio comunale</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="viaCodificata" type="tns:viaCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Via codificata secondo il SIT comunale</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="civico" type="tns:civicoType" minOccurs="0"/>
<xs:element name="esponente" type="tns:esponenteCivicoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Esponente del numero civico</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="piano" type="tns:pianoType" minOccurs="0"/>
<xs:element name="interno" type="tns:internoType" minOccurs="0"/>
<xs:element name="scala" type="tns:scalaType" minOccurs="0"/>
<xs:element name="suffisso" type="tns:suffissoType" minOccurs="0"/>
<xs:element name="km" type="tns:chilometroType" minOccurs="0"/>
<xs:element name="frazione" type="tns:frazioneType" minOccurs="0"/>
<xs:element name="cap" type="tns:capType"/>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiCatastaliNewType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati catastali di un immobile/fabbricato/area</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="sezione" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="3"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="foglio">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{1,4}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="particella">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{1,5}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="subalterno" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{1,4}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="riduzioneTariffaDomestica2014Type">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la riduzione di tariffa del tributo per le utenze domestiche</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:enumeration value="A"/>
<xs:enumeration value="B"/>
<xs:enumeration value="C"/>
<xs:enumeration value="D"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="elencoOccupantiImmobileType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati degli occupanti di un immobile</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="occupanteImmobile" type="tns:occupanteImmobileType" minOccurs="1" maxOccurs="unbounded">
<xs:annotation>
<xs:documentation xml:lang="it">Elenco degli occupanti di un immobile</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="occupanteImmobileType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un occupante di un immobile</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="cognome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="40"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="nome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="40"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="dataNascita" type="tns:dataType"/>
<xs:element name="codiceFiscale" type="tns:codiceFiscaleType"/>
<xs:element name="dataInizioOccupazione" type="tns:dataType" minOccurs="0"/>
<xs:element name="dataFineOccupazione" type="tns:dataType" minOccurs="0"/>
</xs:sequence>
</xs:complexType>

<xs:complexType name="elencoDatiUtenzeCommercialiType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati delle utenze commerciali associabili ad una pratica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="utenza" type="tns:datiUtenzaCommercialeIscrizioneType" minOccurs="1" maxOccurs="unbounded">
<xs:annotation>
<xs:documentation xml:lang="it">Elenco delle utenze commerciali (almeno una) da associare alla pratica</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiUtenzaCommercialeIscrizioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un'utenza commerciale. Specificare obbligatoriamente almeno uno tra supCoperta e supScoperta</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:datiUtenzaBaseType">
<xs:sequence>
<xs:element name="codAttivita" type="tns:codiceAttivitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Classificazione dell'attività economica svolta nel locale/area secondo la codifica Ateco 2007</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneCommerciale" type="tns:riduzioneCommercialeIscrizioneType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Tipologia riduzione richiesta </xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supCoperta" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie coperta tassabile in mq. Da specificare almeno uno tra supCoperta e supScoperta</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supScoperta" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie scoperta tassabile in mq. Da specificare almeno uno tra supCoperta e supScoperta</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supIntassabile" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie intassabile (locali inutilizzabili, impianti fissi, altro, ecc.) in mq</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supRifiutiSpeciali" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Altra superficie intassabile (rifiuti speciali, ecc.) in mq</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:simpleType name="codiceAttivitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la codifica dell'attività economica secondo la classificazione Ateco 2007</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="(0[1-9]|[1-9][0-9])\.[0-9]{2}\.[0-9]{2}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="riduzioneCommercialeIscrizioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati delle possibili riduzioni richiedibili nelle pratiche TARI per le utenze commerciali</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="riduzioneSuperficieArt10" type="tns:riduzioneSuperficieArt10Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 10 Regolamento Tari per Superfici soggette a produzione rifiuti speciali. Possibili valori: 1 = Ambulatori dentistici, laboratori radiologici, laboratori odontotecnici, laboratori di analisi (non facenti parte delle strutture sanitarie che operano in forma organizzata e continuativa nell'ambito e per finalit&amp;agrave; di cui alla legge 833/88 e successive variazioni), 2 = Ambulatori medici (non facenti parte delle strutture sanitarie che operano in forma organizzata e continuativa nell'ambito e per finalit&amp;agrave; di cui alla legge 833/88 e successive variazioni), 3 = Lavanderie a secco e tintorie non industriali, 4 = Distributori di carburanti, 5 = Autofficine per riparazione veicoli, 6 = Gommisti, 7 = Elettrauto, 8 = Caseifici e aziende produttrici di vino e bevande, 9 = Rosticcerie e pasticcerie, 10 = Falegnamerie, 11 = Autocarrozzerie e verniciatori in genere, galvanotecnici, fonderie, produzione di ceramiche, smalterie, 12 = Officine di carpenteria metallica, 13 = Tipografie, stamperie, incisioni e vetrerie, 14 = Laboratori fotografici ed eliografie, 15 = Allestimenti pubblicitari, insegne luminose, 16 = Produzione di plastiche e vetroresine, 17 = Macellerie, 18 = Pescherie, 19 = Parrucchieri, 20 = Imprese di trasporto</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneTariffaArt24" type="tns:riduzioneTariffaArt24Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 24 Regolamento Tari per utenze non domestiche non stabilmente attive. Possibili valori: 10 - Locali, diversi dalle abitazioni, ed aree scoperte adibiti ad uso stagionale o ad uso non continuativo, ma ricorrente, purché non superiore a 183 giorni nell’anno solare (30%)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneTariffaArt26" type="tns:riduzioneTariffaArt26Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 26 Regolamento Tari per inferiori livelli di prestazione del servizio. Possibili valori: 40 - Utenze poste a una distanza superiore a 300 mt. e sino a 500 mt. dal più vicino punto di conferimento, misurata dall'accesso dell'utenza alla strada pubblica (60%), 41 - utenze poste a una distanza superiore a 500 mt. dal più vicino punto di conferimento, misurata dall'accesso dell'utenza alla strada pubblica (70%), 44 - periodi di mancato svolgimento del servizio di gestione dei rifiuti (80%)</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="riduzioneSuperficieArt10Type">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la riduzione ai sensi art. 10 Regolamento Tari per Superfici soggette a produzione rifiuti speciali per le utenze non domestiche. Possibili valori: 1 = Ambulatori dentistici, laboratori radiologici, laboratori odontotecnici, laboratori di analisi (non facenti parte delle strutture sanitarie che operano in forma organizzata e continuativa nell'ambito e per finalit&amp;agrave; di cui alla legge 833/88 e successive variazioni), 2 = Ambulatori medici (non facenti parte delle strutture sanitarie che operano in forma organizzata e continuativa nell'ambito e per finalit&amp;agrave; di cui alla legge 833/88 e successive variazioni), 3 = Lavanderie a secco e tintorie non industriali, 4 = Distributori di carburanti, 5 = Autofficine per riparazione veicoli, 6 = Gommisti, 7 = Elettrauto, 8 = Caseifici e aziende produttrici di vino e bevande, 9 = Rosticcerie e pasticcerie, 10 = Falegnamerie, 11 = Autocarrozzerie e verniciatori in genere, galvanotecnici, fonderie, produzione di ceramiche, smalterie, 12 = Officine di carpenteria metallica, 13 = Tipografie, stamperie, incisioni e vetrerie, 14 = Laboratori fotografici ed eliografie, 15 = Allestimenti pubblicitari, insegne luminose, 16 = Produzione di plastiche e vetroresine, 17 = Macellerie, 18 = Pescherie, 19 = Parrucchieri, 20 = Imprese di trasporto</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:short">
<xs:maxInclusive value="20"/>
<xs:minInclusive value="1"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="riduzioneTariffaArt24Type">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la riduzione ai sensi art. 24 Regolamento Tari per le utenze non domestiche non stabilmente attive. Possibili valori: 10 - Locali, diversi dalle abitazioni, ed aree scoperte adibiti ad uso stagionale o ad uso non continuativo, ma ricorrente, purché non superiore a 183 giorni nell’anno solare (30%)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:enumeration value="10"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="riduzioneTariffaArt26Type">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la riduzione di tariffa ai sensi art. 26 Regolamento Tari per inferiori livelli di prestazione del servizio. Possibili valori: 40 - UTENZE POSTE A UNA DISTANZA SUPERIORE A 300 MT. E SINO A 500 MT. DAL PIU' VICINO PUNTO DI CONFERIMENTO, MISURATA DALL'ACCESSO DELL'UTENZA ALLA STRADA PUBBLICA (60%), 41 - UTENZE POSTE A UNA DISTANZA SUPERIORE A 500 MT. DAL PIU' VICINO PUNTO DI CONFERIMENTO, MISURATA DALL'ACCESSO DELL'UTENZA ALLA STRADA PUBBLICA (70%), 44 - PERIODI DI MANCATO SVOLGIMENTO DEL SERVIZIO DI GESTIONE DEI RIFIUTI (80%)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:enumeration value="40"/>
<xs:enumeration value="41"/>
<xs:enumeration value="44"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="numProtocolloType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un numero di protocollazione di un documento</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{1,11}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiAutorizzazioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di autorizzazione all'accesso di un servizio web Tares</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="identificativoUtente">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo dell'utente (sistema) richiedente il servizio</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{40}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="password">
<xs:annotation>
<xs:documentation xml:lang="it">Password dell'utente (sistema) richiedente il servizio</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{64}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="identificativoServizio">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo del servizio richiesto: S00 - getPinCid, S01 - getUtenze, S02 - setAgevolazioniCAF, S03 - getUtenzeStatoPratiche, S04 - setIscrizione, S05 - setTrasferimento, S06 - setCessazione, S07 - setVariazioneSuperficie, S08 - setVariazioneCategoria, S09 - setRiduzione, S10 - isContribuenteEsistente, S11 - setTrasferimentoCoabitante</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:enumeration value="S00"/>
<xs:enumeration value="S01"/>
<xs:enumeration value="S02"/>
<xs:enumeration value="S03"/>
<xs:enumeration value="S04"/>
<xs:enumeration value="S05"/>
<xs:enumeration value="S06"/>
<xs:enumeration value="S07"/>
<xs:enumeration value="S08"/>
<xs:enumeration value="S09"/>
<xs:enumeration value="S10"/>
<xs:enumeration value="S11"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="dataRichiesta" type="tns:dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Data di richiesta del servizio, nel formato GG/MM/AAAA</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="oraRichiesta">
<xs:annotation>
<xs:documentation xml:lang="it">Ora di richiesta del servizio, nel formato HH:MM:SS</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="([0-1][0-9]|[2][0-3]):([0-5][0-9]):([0-5][0-9]).[0-9]{3}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="idRichiesta">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo della richiesta</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{64}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiTracciamentoRequestType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati utili al tracciamento di una richiesta</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="nomeOperatore">
<xs:annotation>
<xs:documentation xml:lang="it">Denominazione dell'operatore (es. Nome e cognome persona fisica, Denominazione sociale di uno studio commercialista)</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="60"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="codFiscaleOperatore" type="tns:codiceFiscaleType">
<xs:annotation>
<xs:documentation xml:lang="it">Codice fiscale dell'operatore</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="pIvaOperatore" type="tns:partitaIVAType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Partita IVA dell'operatore</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="emailOperatore" type="tns:emailType">
<xs:annotation>
<xs:documentation xml:lang="it">Indirizzo email dell'operatore</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="setIscrizioneResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di risposta del servizio setIscrizione. Codici di ritorno: 0 - Successo, 110 - Richiesta non valida, 999 - Si e' verificato un errore imprevisto nel collegamento con il backend</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="return" type="tns:commonResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati di risposta</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="commonResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati comuni di una risposta di un servizio web Tares</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="idRichiesta">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo della richiesta</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{64}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="code" type="xs:unsignedShort">
<xs:annotation>
<xs:documentation xml:lang="it">Codice di ritorno della risposta del servizio: 100 = Utente e/o password errati, 101 = Utente non abilitato all'uso del servizio, 110 = Richiesta non valida, 111 = Si e' verificato un errore durante la validazione della richiesta, 112 = Richiesta di agevolazione gia' esistente, 120 = L'elaborazione della richiesta non ha prodotto risultati, 999 = Si e' verificato un errore imprevisto nel collegamento con il backend</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="messaggio" type="xs:string" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Messaggio di errore. Presente solo se code diverso da 0</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="setTrasferimentoRequestType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per la dichiarazione di trasferimento di un'utenza</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:commonRequestType">
<xs:sequence>
<xs:element name="datiTrasferimento" type="tns:datiTrasferimentoType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della dichiarazione di trasferimento di un'utenza</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="datiTrasferimentoType">
<xs:annotation>
<xs:documentation xml:lang="it">tipo di dato complesso per i dati di una dichiarazione di trasferimento di un'utenza</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:datiDichiarazioneBaseType">
<xs:sequence>
<xs:element name="datiContribuente" type="tns:datiContribuenteVariazioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati del contribuente dichiarante</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiVecchiaUtenza" type="tns:datiVecchiaUtenzaType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati dell'utenza da trasferire</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice>
<xs:element name="datiNuovaUtenzaCommerciale" type="tns:datiUtenzaCommercialeTrasferimentoType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della nuova utenza</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiNuovaUtenzaDomestica" type="tns:datiNuovaUtenzaDomesticaType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della nuova utenza</xs:documentation>
</xs:annotation>
</xs:element>
</xs:choice>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="datiContribuenteVariazioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un contribuente all'interno di una dichiarazione di variazione (cessazione, trasferimento, variazione, riduzione) di un'utenza</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="idContribuente" type="tns:identificativoContribuenteType">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice minOccurs="0">
<xs:element name="variazioneAnagraficaPersonaFisica" type="tns:variazioneAnagraficaPersonaFisicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati anagrafici della persona fisica o giuridica in caso di variazione. E' possibile specificarli solo in caso di esistenza del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="variazioneAnagraficaPersonaGiuridica" type="tns:variazioneAnagraficaPersonaGiuridicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati anagrafici della persona fisica o giuridica in caso di variazione. E' possibile specificarli solo in caso di esistenza del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
</xs:choice>
<xs:element name="datiDomicilio" type="tns:indirizzoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della residenza anagrafica (se persona fisica) o della sede anagrafica (persona giuridica). Da specificare solo se variati e si richiede l'aggiornamento degli archivi. Se comune è Bari allora specificare obbligatoriamente viaCodificata altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiRappresentante" type="tns:datiRappresentanteType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della persona fisica rappresentante legalmente una persona giuridica. Da specificare solo in caso di contribuente di tipo persona giuridica, se essi sono variati e se ne richiede l'aggiornamento</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiRecapito" type="tns:datiRecapitoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati dell'eventuale recapito (differente dalla residenza anagrafica o sede legale) del contribuente per la ricezione delle comunicazioni. Da specificare solo se variati e si richiede l'aggiornamento degli archivi. Se comune è Bari allora specificare obbligatoriamente viaCodificata altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="indirizzoPEC" type="tns:emailType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Indirizzo di Posta Elettronica Certificata (P.E.C.) del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="telefono" type="tns:telefonoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di telefono di contatto del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="fax" type="tns:telefonoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Numero di fax di contatto del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="variazioneAnagraficaPersonaFisicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati anagrafici di una persona fisica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="cognome" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="30"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="nome" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="24"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="comuneNascita" type="tns:comuneNascitaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Comune di nascita codificato tramite il codice ISTAT o se nato all'estero codifica ISTAT dello stato estero</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="comuneEsteroNascita" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione del comune estero di nascita. Da specificare solo se nato all'estero</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="30"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="provinciaNascita" type="tns:siglaProvinciaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Provincia di nascita. Sarà presente solo se nato in Italia</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="dataNascita" type="tns:dataType" minOccurs="0"/>
<xs:element name="sesso" type="tns:sessoType" minOccurs="0"/>
<xs:element name="codiceFiscale" type="tns:codiceFiscaleType" minOccurs="0"/>
<xs:element name="partitaIVA" type="tns:partitaIVAType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Partita I.V.A. dell'attività economica svolta</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="variazioneAnagraficaPersonaGiuridicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati anagrafici di una persona giuridica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="denominazione" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Denominazione o ragione sociale</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="84"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="codiceFiscale" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Codice fiscale della persona giuridica</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{11}|[a-zA-Z]{6}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){3}[a-zA-Z]{1}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="partitaIVA" type="tns:partitaIVAType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Partita I.V.A. dell'attività economica svolta</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiVecchiaUtenzaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un'utenza da trasferire</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="idUtenza" type="tns:idUtenzaType">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo utenza</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="dataVariazione" type="tns:dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Data di variazione dell'utenza nel formato gg/mm/aaaa</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="dataCessazione" type="tns:dataType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Data di cessazione dell'utenza nel formato gg/mm/aaaa. Da specificare se presente discontinuità tra i periodi di possesso/occupazione delle utenze vecchia e nuova</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="idUtenzaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per l'identificativo di un'utenza</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:int">
<xs:maxInclusive value="999"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiUtenzaCommercialeTrasferimentoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un'utenza commerciale. Specificare obbligatoriamente almeno uno tra supCoperta e supScoperta</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:datiUtenzaBaseType">
<xs:sequence>
<xs:element name="codAttivita" type="tns:codiceAttivitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Classificazione dell'attività economica svolta nel locale/area secondo la codifica Ateco 2007</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneCommerciale" type="tns:riduzioneCommercialeTrasferimentoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Tipologia riduzione richiesta </xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supCoperta" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie coperta tassabile in mq. Da specificare almeno uno tra supCoperta e supScoperta</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supScoperta" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie scoperta tassabile in mq. Da specificare almeno uno tra supCoperta e supScoperta</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supIntassabile" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Superficie intassabile (locali inutilizzabili, impianti fissi, altro, ecc.) in mq</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="supRifiutiSpeciali" type="tns:superficieType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Altra superficie intassabile (rifiuti speciali, ecc.) in mq</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="riduzioneCommercialeTrasferimentoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati delle possibili riduzioni richiedibili nelle pratiche TARI per le utenze commerciali</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="riduzioneSuperficieArt10" type="tns:riduzioneSuperficieArt10Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 10 Regolamento Tari per Superfici soggette a produzione rifiuti speciali. Possibili valori: 1 = Ambulatori dentistici, laboratori radiologici, laboratori odontotecnici, laboratori di analisi (non facenti parte delle strutture sanitarie che operano in forma organizzata e continuativa nell'ambito e per finalit&amp;agrave; di cui alla legge 833/88 e successive variazioni), 2 = Ambulatori medici (non facenti parte delle strutture sanitarie che operano in forma organizzata e continuativa nell'ambito e per finalit&amp;agrave; di cui alla legge 833/88 e successive variazioni), 3 = Lavanderie a secco e tintorie non industriali, 4 = Distributori di carburanti, 5 = Autofficine per riparazione veicoli, 6 = Gommisti, 7 = Elettrauto, 8 = Caseifici e aziende produttrici di vino e bevande, 9 = Rosticcerie e pasticcerie, 10 = Falegnamerie, 11 = Autocarrozzerie e verniciatori in genere, galvanotecnici, fonderie, produzione di ceramiche, smalterie, 12 = Officine di carpenteria metallica, 13 = Tipografie, stamperie, incisioni e vetrerie, 14 = Laboratori fotografici ed eliografie, 15 = Allestimenti pubblicitari, insegne luminose, 16 = Produzione di plastiche e vetroresine, 17 = Macellerie, 18 = Pescherie, 19 = Parrucchieri, 20 = Imprese di trasporto</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneTariffaArt24" type="tns:riduzioneTariffaArt24Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 24 Regolamento Tari per utenze non domestiche non stabilmente attive. Possibili valori: 10 - Locali, diversi dalle abitazioni, ed aree scoperte adibiti ad uso stagionale o ad uso non continuativo, ma ricorrente, purché non superiore a 183 giorni nell’anno solare (30%)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneTariffaArt26" type="tns:riduzioneTariffaArt26Type" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 26 Regolamento Tari per inferiori livelli di prestazione del servizio. Possibili valori: 40 - Utenze poste a una distanza superiore a 300 mt. e sino a 500 mt. dal più vicino punto di conferimento, misurata dall'accesso dell'utenza alla strada pubblica (60%), 41 - utenze poste a una distanza superiore a 500 mt. dal più vicino punto di conferimento, misurata dall'accesso dell'utenza alla strada pubblica (70%), 44 - periodi di mancato svolgimento del servizio di gestione dei rifiuti (80%)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneTariffaArt27" type="tns:riduzioneTariffaArt27CommercialeType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Riduzione ai sensi art. 27 Regolamento Tari. 5 = Possibili valori: Locali e aree utilizzati da Enti e Associazioni riconosciuti per legge come enti morali aventi finalità di assistenza e beneficenza (100%), 20 = Aree scoperte destinate alla commercializzazione di autoveicoli ovvero ad altra attività avente analoga produttività di rifiuti, con superficie almeno pari a 1.000 metri quadrati (30%), 45 = Locali individuati dal Comune ed affidati a Onlus, per la realizzazione dei servizi sociali (100%)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="revocaRiduzione" type="xs:boolean" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Indica se e' richiesta la revoca della riduzione in atto</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="riduzioneTariffaArt27CommercialeType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la riduzione di tariffa ai sensi art. 27 Regolamento Tari. Possibili valori: 5 = Locali e aree utilizzati da Enti e Associazioni riconosciuti per legge come enti morali aventi finalità di assistenza e beneficenza (100%), 20 = Aree scoperte destinate alla commercializzazione di autoveicoli ovvero ad altra attività avente analoga produttività di rifiuti, con superficie almeno pari a 1.000 metri quadrati (30%), 45 = Locali individuati dal Comune ed affidati a Onlus, per la realizzazione dei servizi sociali (100%)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:enumeration value="5"/>
<xs:enumeration value="20"/>
<xs:enumeration value="45"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiNuovaUtenzaDomesticaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di una utenza domestica associabile ad una pratica di trasferimento</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="nuovaUtenza" type="tns:datiUtenzaDomesticaType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della nuova utenza domestica da associare alla pratica</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="contribuenteResidente" type="xs:boolean">
<xs:annotation>
<xs:documentation xml:lang="it">Indica se il contribuente risiede nell'immobile o meno. E' riferito alle utenze domestiche specificate</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="riduzioneDomestica" type="tns:riduzioneDomesticaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Codice di riduzione tariffa richiesta ai sensi degli articoli 23 e 27 del Regolamento Tari</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="numComponentiNucleoFamiliare">
<xs:simpleType>
<xs:restriction base="xs:unsignedByte">
<xs:fractionDigits value="0"/>
<xs:maxInclusive value="20"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="elencoOccupantiImmobile" type="tns:elencoOccupantiImmobileVariazioneType" minOccurs="0">
<xs:annotation>
